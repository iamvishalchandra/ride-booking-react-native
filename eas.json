{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "apk-dev": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
