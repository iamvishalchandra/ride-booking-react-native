{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@component/*": ["./src/component/*"],
      "@constant/*": ["./src/constant/*"],
      "@data/*": ["./src/data/*"],
      "@helper/*": ["./src/helper/*"],
      "@store/*": ["./src/store*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
